<div class="container">
  <h1>Add new Cocktail</h1>
  <p>Add a delicious cocktail</p>

  <%= simple_form_for [@cocktail] do |f| %>
  <%= f.input :name %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit "Add new cocktail", class: "btn btn-primary" %>
  <% end %>

 <!--  <%#= cl_image_tag("otaaj35yqecduhpwxzcq",
      width: 400, height: 300, crop: :fill) %> -->
</div>
